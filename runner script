local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
hrp.CFrame = CFrame.new(-16268.0479, 21.5549812, 1373.18726, -0.423662543, -0.415843904, -0.804725945, 0, 0.888394833, -0.45908004, 0.905820072, -0.194495022, -0.376379639)
