button.MouseButton1Click:Connect(function()
local broken = 39


while wait() do
if broken == 20 then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if broken == 40 then
break
end
local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
hrp.CFrame = CFrame.new(4025.26416, -1.49270487, -1819.59558, 0.490086347, 0.0490992069, 0.870290041, 1.86264493e-09, 0.99841243, -0.0563274957, -0.871673882, 0.027605338, 0.489308298)
broken = broken + 1
end
end)

button1.MouseButton1Click:Connect(function()
local broken = 50


while wait() do
if broken == 40 then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if broken == 51 then
break
end
local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
hrp.CFrame = CFrame.new(-6500.91455, 79.9662857, -123.177612, 0.975223184, 0.040786121, 0.217431277, 0, 0.982857764, -0.184366062, -0.221223578, 0.179798052, 0.958505511)
broken = broken + 1
end
end)

button2.MouseButton1Click:Connect(function()
local broken = 0


