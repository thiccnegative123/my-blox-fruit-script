button6.MouseButton1Click:Connect(function()
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer

-- SETTINGS
local COOLDOWN_PER_CHEST = 0.2
local CHECK_DELAY = 0.1
local BUTTON_OFFSET_Y = 0.07

local chestFolder = workspace:WaitForChild("ChestModels")

local running = false
local loopThread

-- PC only
if UserInputService.KeyboardEnabled then
	local gui = Instance.new("ScreenGui")
	gui.Name = "ChestTPGui"
	gui.ResetOnSpawn = false
	gui.Parent = player:WaitForChild("PlayerGui")

	local button = Instance.new("TextButton")
	button.Size = UDim2.fromOffset(110, 35)
	button.Position = UDim2.fromScale(1, BUTTON_OFFSET_Y)
	button.AnchorPoint = Vector2.new(1, 0)
	button.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	button.BorderSizePixel = 0
	button.Text = "TP Chest"
	button.TextColor3 = Color3.fromRGB(255, 200, 0)
	button.Font = Enum.Font.GothamBold
	button.TextSize = 14
	button.Parent = gui

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0, 8)
	corner.Parent = button

	local function startLoop()
		loopThread = task.spawn(function()
			while running do
				local char = player.Character
				local hrp = char and char:FindFirstChild("HumanoidRootPart")

				if hrp then
					for _, chest in ipairs(chestFolder:GetChildren()) do
						if not running then break end
						if chest:IsA("Model") and chest.PrimaryPart then
							hrp.CFrame = chest.PrimaryPart.CFrame
							task.wait(COOLDOWN_PER_CHEST)
						end
					end
				end

				task.wait(CHECK_DELAY)
			end
		end)
	end

	button.MouseButton1Click:Connect(function()
		running = not running

		if running then
			button.Text = "STOP TP"
			startLoop()
		else
			button.Text = "TP Chest"
		end
	end)
end

end)

